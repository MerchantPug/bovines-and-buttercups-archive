      "emi": "net.merchantpug.bovinesandbuttercups.client.integration.emi.BovineEmiPlugin",
      "preLaunch": "com.llamalad7.mixinextras.MixinExtrasBootstrap::init"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=4.0.0-"
      },
      {
        "id": "minecraft",
        "versions": "1.19.2"
      }
    ]
  },
  "cardinal-components": [
    "bovinesandbuttercups:lockdown",
    "bovinesandbuttercups:moobloom_target",
    "bovinesandbuttercups:mooshroom_type"
