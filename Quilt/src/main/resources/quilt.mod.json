{
  "schema_version": 1,
  "quilt_loader": {
    "group": "net.merchantpug",
    "id": "bovinesandbuttercups",
    "version": "${version}",
    "metadata": {
      "name": "Bovines and Buttercups",
      "description": "A new take on the Moobloom and other bovines.",
      "license": "LGPL-3.0-only",
      "contributors": {
        "MerchantPug": "Owner",
        "SpiderKolo": "Artist"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/bovines-and-buttercups",
        "issues": "https://github.com/MerchantPug/bovines-and-buttercups/issues",
        "sources": "https://github.com/MerchantPug/bovines-and-buttercups"
      },
      "icon": "assets/bovinesandbuttercups/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "net.merchantpug.bovinesandbuttercups.BovinesAndButtercupsQuilt",
      "client_init": "net.merchantpug.bovinesandbuttercups.client.BovinesAndButtercupsClientQuilt",
